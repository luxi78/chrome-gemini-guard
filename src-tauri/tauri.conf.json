{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "chrome-gemini-guard",
  "version": "1.0.3",
  "identifier": "com.lx.chromegeminiguard",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Chrome Gemini Guard",
        "width": 848,
        "height": 544,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}